{
    "name": "spryker-eco/minubo",
    "type": "library",
    "description": "Minubo module",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "spryker/flysystem-aws3v3-file-system": "^2.0.0",
        "spryker/file-system": "^2.0.0",
        "spryker/oms": "^8.1.0 || ^10.0.0 || ^11.0.0",
        "spryker/sales": "^8.0.0 || ^10.0.0 || ^11.0.0",
        "spryker/util-encoding": "^2.0.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0.0",
        "spryker/code-sniffer": "^0.17.1",
        "spryker/config": "^3.5.0",
        "spryker/monolog": "^2.0.4",
        "spryker/propel": "^3.29.0",
        "spryker/queue": "^1.5.0",
        "spryker/testify": "^3.40.0",
        "spryker/transfer": "^3.25.0"
    },
    "autoload": {
        "psr-4": {
            "SprykerEco\\": "src/SprykerEco/",
            "SprykerEcoTest\\": "tests/SprykerEcoTest"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 600
    }
}
