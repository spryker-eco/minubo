    "require": {
        "php": ">=7.1",
        "spryker/flysystem-aws3v3-file-system": "^1.0.0",
        "spryker/file-system": "^1.0.0",
        "spryker/oms": "^8.1.0 || ^10.0.0 || ^11.0.0",
